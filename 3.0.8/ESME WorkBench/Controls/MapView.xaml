<UserControl x:Class="ESMEWorkbench.Controls.MapView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Map="clr-namespace:ESMEWorkbench.ViewModels.Map"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:WpfDesktopEdition="clr-namespace:ThinkGeo.MapSuite.WpfDesktopEdition;assembly=WpfDesktopEdition"
             AllowDrop="True"
             d:DataContext="{d:DesignInstance Map:MapViewModel}"
             mc:Ignorable="d">
    <Grid>
        <WpfDesktopEdition:WpfMap x:Name="WpfMap" />
    </Grid>
</UserControl>