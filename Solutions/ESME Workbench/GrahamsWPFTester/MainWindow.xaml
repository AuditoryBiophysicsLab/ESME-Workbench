<Window x:Class="GrahamsWPFTester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mef="http:\\www.codeplex.com\MEFedMVVM"
        xmlns:p="clr-namespace:ESME.Views.PSM;assembly=ESME.Views"
        Title="{Binding WindowTitle}"
        mef:ViewModelLocator.ViewModel="MainWindowViewModel"
        ResizeMode="NoResize"
        WindowStyle="ToolWindow"
        SizeToContent="WidthAndHeight">
    <Grid>
        <p:PSMTreeView DataContext="{Binding TreeViewModel}" />
    </Grid>
</Window>
