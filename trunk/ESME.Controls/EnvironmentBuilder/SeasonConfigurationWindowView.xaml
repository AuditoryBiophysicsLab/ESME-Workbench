<Window x:Class="ESME.Views.EnvironmentBuilder.SeasonConfigurationWindowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:View="clr-namespace:ESME.Views.EnvironmentBuilder"
        
        Title="SeasonConfigurationWindowView" Height="400" Width="300">
    <DockPanel LastChildFill="True">
        <StackPanel DockPanel.Dock="Bottom">
        <Button DockPanel.Dock="Bottom"
                        VerticalAlignment="Bottom"
                        Margin="5"
                        Command="{Binding OkCommand}">
            Set Season Definitions
        </Button>
        </StackPanel>
        <StackPanel>
            <View:SeasonConfigurationView/>
        </StackPanel>

    </DockPanel>
</Window>
